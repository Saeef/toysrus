{
  "manifest_version": 2,
  "name": "Quidco - 05 - Landing Page",
  "description": "#54215412",
  "version": "1.2.5",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app/jquery.js","app/core.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "app/icon.png",
    "default_title": "Quidco - 05 - Landing Page",
    "default_popup": "app/extension.html"
  },
  "icons": {
    "128": "app/img/icon128.png"
  },
  "permissions": [
    "activeTab",
    "<all_urls>",
    "cookies"
  ],
  "web_accessible_resources": [
    "styles/_reset.less",
    "styles/_component.live-table.less",
    "styles/_grids.less",
    "styles/_buttons.less",
    "styles/*.less",
    "v1.js",
    "v2.js",
    "v3.js",
    "app/extra/*.js",
    "app/less.js",
    "app/jquery.js",
    "includes/header_js.js",
    "includes/footer_js.js"
  ]
}
